\u05de\u05d9\u05dd',\r\n                        '\u05e8\u05d0\u05e9\u05d5\u05df-\u05dc\u05e6\u05d9\u05d5\u05df'\r\n                    ],\r\n                    callName: firstName && lastName ? `${firstName} ${lastName}` : '\u05d0\u05d5\u05e8\u05d7'\r\n                });\r\n                \r\n                console.log('\ud83d\udcdd User identity updated');\r\n            }, 1000);\r\n        }\r\n        \r\n        \/\/ \u05d4\u05d5\u05e1\u05e3 event listeners \u05dc\u05db\u05dc \u05d4\u05e9\u05d3\u05d5\u05ea\r\n        ['firstName', 'lastName', 'email', 'phone', 'idNumber'].forEach(fieldId => {\r\n            document.getElementById(fieldId).addEventListener('input', updateUserIdentity);\r\n        });\r\n        \r\n        \/\/ ============================================\r\n        \/\/ \u05e4\u05d5\u05e0\u05e7\u05e6\u05d9\u05d5\u05ea Modal\r\n        \/\/ ============================================\r\n        function showModal() {\r\n            if (sessionActive) {\r\n                alert('\u2705 \u05e1\u05e9\u05df \u05db\u05d1\u05e8 \u05e4\u05e2\u05d9\u05dc - \u05e0\u05e6\u05d9\u05d2 \u05e8\u05d5\u05d0\u05d4 \u05d0\u05ea \u05d4\u05de\u05e1\u05da \u05e9\u05dc\u05da');\r\n                return;\r\n            }\r\n            if (helpRequested) {\r\n                alert('\u23f3 \u05d1\u05e7\u05e9\u05ea \u05e2\u05d6\u05e8\u05d4 \u05db\u05d1\u05e8 \u05e0\u05e9\u05dc\u05d7\u05d4 - \u05de\u05de\u05ea\u05d9\u05df \u05dc\u05e0\u05e6\u05d9\u05d2...');\r\n                return;\r\n            }\r\n            document.getElementById('confirmModal').classList.add('show');\r\n        }\r\n        \r\n        function closeModal() {\r\n            document.getElementById('confirmModal').classList.remove('show');\r\n        }\r\n        \r\n        \/\/ \u05e1\u05d2\u05d9\u05e8\u05ea Modal \u05d1\u05dc\u05d7\u05d9\u05e6\u05d4 \u05de\u05d7\u05d5\u05e5 \u05dc\u05ea\u05d5\u05db\u05df\r\n        document.getElementById('confirmModal').addEventListener('click', function(e) {\r\n            if (e.target === this) {\r\n                closeModal();\r\n            }\r\n        });\r\n        \r\n        \/\/ ============================================\r\n        \/\/ \u05d1\u05e7\u05e9\u05ea \u05e2\u05d6\u05e8\u05d4 - \u05d4\u05e4\u05d5\u05e0\u05e7\u05e6\u05d9\u05d4 \u05d4\u05de\u05e8\u05db\u05d6\u05d9\u05ea!\r\n        \/\/ ============================================\r\n        function requestHelp() {\r\n            closeModal();\r\n            \r\n            \/\/ \u05e1\u05de\u05df \u05e9\u05d1\u05d9\u05e7\u05e9\u05e0\u05d5 \u05e2\u05d6\u05e8\u05d4\r\n            helpRequested = true;\r\n            \r\n            \/\/ \ud83c\udfaf \u05d6\u05d4 \u05d4\u05d7\u05dc\u05e7 \u05d4\u05db\u05d9 \u05d7\u05e9\u05d5\u05d1!\r\n            \/\/ requestAgent() \u05d2\u05d5\u05e8\u05dd \u05dc\u05de\u05e9\u05ea\u05de\u05e9 \u05dc\u05d4\u05d5\u05e4\u05d9\u05e2 \u05d1\u05d3\u05e9\u05d1\u05d5\u05e8\u05d3 \u05e2\u05dd \u05e1\u05d9\u05de\u05d5\u05df \"\u05de\u05d1\u05e7\u05e9 \u05e2\u05d6\u05e8\u05d4\"\r\n            Upscope('requestAgent');\r\n            \/\/Upscope('requestCall');\r\n            \/\/ \u05e2\u05d3\u05db\u05df \u05d0\u05ea \u05d4\u05db\u05e4\u05ea\u05d5\u05e8 \u05dc\u05de\u05e6\u05d1 \u05d4\u05de\u05ea\u05e0\u05d4\r\n            updateButtonState('waiting');\r\n            \r\n            \/\/ \u05d4\u05e6\u05d2 \u05d0\u05ea \u05e7\u05d5\u05d3 \u05d4\u05d7\u05d9\u05e4\u05d5\u05e9\r\n            showLookupCode();\r\n            \r\n            console.log('\ud83c\udd98 Help requested - You are now visible in the agent dashboard!');\r\n        }\r\n        \r\n        \/\/ ============================================\r\n        \/\/ \u05e2\u05d3\u05db\u05d5\u05df UI\r\n        \/\/ ============================================\r\n        function updateButtonState(state) {\r\n            const btn = document.getElementById('helpBtn');\r\n            btn.classList.remove('waiting', 'active');\r\n            \r\n            if (s